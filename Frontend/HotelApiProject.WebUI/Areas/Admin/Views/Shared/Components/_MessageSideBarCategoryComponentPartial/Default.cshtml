@model List<MessageCategoryListDto>
<h5 class="mt-5 m-b-10">Kategoriler</h5>
<div class="list-group mail-list">
    @foreach (var item in Model)
    {
          <a href="/Admin/Contact/MessageListByCategory/@item.MessageCategoryId" class="list-group-item border-0"><i class="@item.IconUrl"></i>  @item.MessageCategoryName</a>
    }
</div>