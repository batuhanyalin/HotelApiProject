
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div class="row page-titles mx-0">
    <div class="col p-md-0">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/Admin/Dashboard/Index">Anasayfa</a></li>
            <li class="breadcrumb-item active"><a href="javascript:void(0)">Yeni Dosya Ekle</a></li>
        </ol>
    </div>
</div>
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-6">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Dosya Yükleme İşlemi</h4>
                    <div class="basic-form">
                        <form enctype="multipart/form-data" method="post">
                            <div class="form-row">
                                <div class="form-group col">
                                    <label>Yüklemek istediğiniz dosyayı seçin</label>
                                    <input type="file" name="file" class="form-control"  />
                                </div>
                            </div>
                            <button class="btn mb-1 btn-success">
                                Dosyayı Yükle <span class="btn-icon-right"><i style="font-size:larger" class="fas fa-file-upload"></i></span>
                            </button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

